@model ProductListModel
@{
    ViewData["Title"] = "List";
}


<div class="row">
    <div class="col-md-3 mt-4">
        <ul>
            <li class=" list-group-item">Erkek </li>
            <li class=" list-group-item">Kadın </li>
            <li class=" list-group-item">Çocuk</li>
        </ul>
    </div>
    <div class="col-md-9 mt-4">
        <div class="row ">
            @foreach (var productItem in Model.Products)
            {
                <partial name="_product" for="@productItem" />
            }
        </div>
    </div>
</div>

